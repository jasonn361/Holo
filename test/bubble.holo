BubbleSort: void (arr: [int], n: int){
    i, j: int;
    swapped: bool;

    repeat (i = 0; i < n - 1; i += 1) {
        swapped = false;
        repeat (j = 0; j < n - i - 1; j += 1) {
            if (arr[j] > arr[j + 1]) {
                swap(arr[j], arr[j + 1]);
                swapped = true;
            }
        }

        if (swapped == false)
            end;
    }
}